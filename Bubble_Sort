#include<stdio.h>
int n,arr[30],i,j,temp;
void bubblesort()
{
	for(i=0;i<n-1;i++)
	{
		for(j=0;j<=n-1-1;j++)
		{
			if(arr[j]>arr[j+1])
			{
				temp=arr[j];
				arr[j]=arr[j+1];
				arr[j+1]=temp;
			}
		}
	}
}
void display()
{
	printf("\nSorted elements are : ");
	for(i=0;i<n;i++)
	{
		printf("%d ",arr[i]);
	}
}
main()
{
	printf("\nEnter the no. of element : ");
	scanf("%d",&n);
	printf("\nEnter the elements : ");
	for(i=0;i<n;i++)
	{
		scanf("%d",&arr[i]);
	}
	bubblesort();
	display();
}
